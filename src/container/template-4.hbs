


    <header class="header">
        <div class="">
        {{> logo}}
        </div>

        <nav class="header_nav">
            <div class="header_nav_right">
            {{>nav list=header}}
            </div>
        </nav>
    </header>


{{#with main}}
<div class="main">
    <img src="{{image}}" alt="main image" class="main_image">
    <div class="main-content">
            <h1 class="main_title">{{heading}}</h1>
            <p class="main_text">{{descriotion}}</p>
        <a class="main_button" href="{{href}}">{{button}}</a> </div>
    </div>
{{/with}}

<main class="featured">
    {{#each featured}}
        {{> cardFeatured data=this}}
    {{/each}}
</main>


<div class="action">
    {{#each actions}}
    <div class="action_content">
        <h5 class="action_title">{{heading}}</h5>
        <a href="{{url}}" class="button action_button">{{text}}</a>
        </div>
        {{/each}}
    </div>

<footer class="footer">
    <div>
            {{> logo}}
        <span class="rooter_copyright">Copyright 2023-2024</span>
    </div>
            {{#each footer}}
            <div>
                <span class="footer_title"> {{title}}</span>
                {{>nav list=links isVertical=true}}
            </div>
            {{/each}}
</footer>

